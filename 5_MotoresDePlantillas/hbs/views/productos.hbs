
  <a href="/"> Index</a>
  <h1>Productos</h1>
  {{#if listExists}}
  <div class="table-responsive px-4">
    <table class="table table-dark">
      <tr style="color: yellow;"> <th>Title</th> <th>Price</th> <th>Thumbnail</th> </tr>
      {{#each productos}}
        <tr> 
          <td>{{this.title}}</td>
          <td>{{this.price}} </td>
          <td>{{this.thumbnail}}</td>
        </tr>
      {{/each}}
    </table>
  </div>
  {{else}}
  <p>No se encontraron productos</p>
  {{/if}}